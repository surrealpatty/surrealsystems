{
  "name": "codecrowds",
  "version": "1.0.0",
  "description": "CodeCrowds is a Fiverr-like platform where users can sell their skills, post services, and hire others for tasks.",
  "main": "src/index.js",
  "type": "commonjs",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:status": "npx sequelize-cli db:migrate:status",
    "test": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles --forceExit"
  },
  "engines": {
    "node": "20.x"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.81.1",
    "bcryptjs": "^3.0.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "stripe": "^19.1.0"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.6.2",
    "sequelize-cli": "^6.6.3",
    "supertest": "^6.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/surrealpatty/CodeCrowds.git"
  },
  "keywords": [
    "fiverr",
    "freelance",
    "marketplace",
    "nodejs",
    "postgresql",
    "sequelize",
    "express"
  ],
  "author": "Patty U.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/surrealpatty/CodeCrowds/issues"
  },
  "homepage": "https://github.com/surrealpatty/CodeCrowds#readme"
}
